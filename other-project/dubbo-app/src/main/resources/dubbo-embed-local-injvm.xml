<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd        
	http://code.alibabatech.com/schema/dubbo        
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


    <dubbo:application name="spring-dubbo-wanli-embed"/>
    <dubbo:protocol name="injvm"  />
    <dubbo:registry protocol="zookeeper" address="zookeeper://127.0.0.1:2181" client="curator"/>
    <dubbo:provider protocol="injvm"  timeout="3000" threads="10" retries="0"></dubbo:provider>

    <!--<dubbo:application name="spring-dubbo-wanli-embed"/>-->
    <!--<dubbo:protocol name="dubbo" port="8098"/>-->
    <!--<dubbo:registry address="zookeeper://127.0.0.1:2181"/>-->
    <!--<dubbo:provider timeout="3000" threads="10" retries="0"></dubbo:provider>-->

    <import resource="dubbo-config1.xml"/>
    <import resource="dubbo-config2.xml"/>
</beans>
